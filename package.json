{
  "name": "ui-publishing-event",
  "version": "1.0.16",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "pre-deploy": "chmod +x pre-deploy.sh && sh pre-deploy.sh",
    "deploy:ios": "yarn build && npm version patch --no-git-tag-version && chmod +x pre-deploy-ios.sh && sh pre-deploy-ios.sh && git add . && git commit -m \"deploy $npm_package_version\" && git push && gh-pages -d dist",
    "deploy:window": "yarn build && npm version patch --no-git-tag-version && chmod +x pre-deploy.sh && sh pre-deploy.sh && git add . && git commit -m \"deploy %npm_package_version%\" && git push && gh-pages -d dist"
  },
  "dependencies": {
    "ant-design-vue": "4.x",
    "dayjs": "^1.11.13",
    "file-saver": "^2.0.5",
    "pinia": "^2.1.7",
    "vue": "^3.4.29",
    "vue-grid-layout": "^2.4.0",
    "vue-router": "^4.3.3",
    "vue3-google-login": "^2.0.33",
    "vue3-grid-layout-next": "^1.0.7"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/file-saver": "^2.0.7",
    "@types/node": "^20.14.5",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "gh-pages": "^6.1.1",
    "npm-run-all2": "^6.2.0",
    "postcss": "^8.4.47",
    "prettier": "^3.2.5",
    "sass-embedded": "^1.79.2",
    "tailwindcss": "^3.4.12",
    "typescript": "~5.4.0",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^5.3.1",
    "vite-plugin-vue-devtools": "^7.3.1",
    "vue-tsc": "^2.0.21"
  }
}
